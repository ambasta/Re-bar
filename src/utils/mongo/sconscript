import os

Import('env')
env = env.Clone()

libraries = list()

lib = env.StaticLibrary('mongo_base', ['base.cxx'])
libraries.append(lib)
env.Append(LIBS=libraries)

libraries = list()

lib = env.StaticLibrary('mongo_read', ['reader.cxx'])
libraries.append(lib)

lib = env.StaticLibrary('mongo_write', ['writer.cxx'])
libraries.append(lib)

Return("libraries")
